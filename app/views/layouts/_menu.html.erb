<ul id="nav">
  <% [Patient, Place, ConsultationPrice, LaboratoryProfile, Diagnostic,
  MedicalCertificate, MedicalOrder, Drug, HospitalExpense, SurgicalStaff,
  Income, VaccinesName, Vaccine, VaccineSale].each do |model| %>
    <% @controller.controller_name == model.name.pluralize.underscore ? style = "selected" : style = "dude" %>
    <li class="<%= style %>">
      <%= link_to model.human_name, :controller => model.name.pluralize.underscore %>
    </li>
  <% end %>
</ul>