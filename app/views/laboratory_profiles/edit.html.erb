<h1>Editing laboratory_profile</h1>

<% form_for(@laboratory_profile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @laboratory_profile %> |
<%= link_to 'Back', laboratory_profiles_path %>